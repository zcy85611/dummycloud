From d5a957a92ba8ff711847f05c0fa749e60ac92607 Mon Sep 17 00:00:00 2001
From: doebi <christoph@doeberl.at>
Date: Fri, 27 Oct 2023 23:16:08 +0200
Subject: [PATCH 06/18] allow git head as version

---
 Makefile       | 2 +-
 dockerbuild.sh | 2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/Makefile b/Makefile
index bcd73601..c7bdddb6 100644
--- a/Makefile
+++ b/Makefile
@@ -1,4 +1,4 @@
-VERSION :=$(shell git describe --tags)
+VERSION :=$(shell git describe --tags --always)
 LDFLAGS := "-s -w -X main.version=$(VERSION)"
 OUT_DIR := dist
 CMD := ./cmd/rmfakecloud
diff --git a/dockerbuild.sh b/dockerbuild.sh
index c8a66c3b..925b5a3b 100755
--- a/dockerbuild.sh
+++ b/dockerbuild.sh
@@ -1,2 +1,2 @@
 export DOCKER_BUILDKIT=1
-docker build -t rmfakecloud --no-cache --build-arg VERSION="$(git describe --tags)" .
+docker build -t rmfakecloud --no-cache --build-arg VERSION="$(git describe --tags --always)" .

